.start "Use?" {
	_useyes "Yes" {
		++
		#caca
	}
	_useno "No!" {
		#nouse
	}
}
.caca "Menuda caca" {
	_comecaca "Comerte tu caca" {
		++
	}
	_nocomecaca "No comerte tu caca" {
		--
	}
}
.nouse "No te comes tu caca, seguro?" {
	_nouseyes = "Si" {
		--
	}
	_nouseno = "No" {
		++
	}
}